{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/movedrop.php"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, X-API-KEY" }
      ]
    }
  ]
}
